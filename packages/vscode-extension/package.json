{
  "name": "framework-helper",
  "displayName": "vscode-extension",
  "publisher": "swordjs",
  "description": "",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:Route"
  ],
  "main": "./lib/index.cjs",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "swordjs-framework-helper",
          "title": "swordjs-framework-helper",
          "icon": "media/logo.svg"
        }
      ]
    },
    "views": {
      "swordjs-framework-helper": [
        {
          "id": "Route",
          "name": "Route",
          "icon": "media/logo.svg",
          "contextualTitle": "Package Explorer"
        }
      ]
    },
    "commands": [
      {
        "command": "Route.refreshEntry",
        "title": "Refresh",
        "icon": {
          "light": "media/refresh_light.svg",
          "dark": "media/refresh_dark.svg"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "Route.refreshEntry",
          "when": "view == Route",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "dev": "nodemon --watch src/ -C -e ts --debug -x 'npm run build'",
    "build": "unbuild",
    "test": "vitest run"
  },
  "devDependencies": {
    "@types/vscode": "^1.69.0",
    "@types/glob": "^7.2.0",
    "@vscode/test-electron": "^2.1.5",
    "unbuild": "^0.6.9"
  },
  "unbuild": {
    "entries": [
      "./src/index"
    ],
    "outDir": "lib",
    "rollup": {
      "emitCJS": true,
      "cjsBridge": true
    }
  }
}
