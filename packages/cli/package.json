{
  "name": "@sword-code-practice/backend-cli",
  "version": "0.0.1-rc.18",
  "description": "",
  "types": "./lib/index.d.ts",
  "module": "./lib/index.mjs",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "unbuild --stub",
    "dev:nodemon": "nodemon --watch src/ -C -e ts --debug -x 'npm run async'",
    "prettier": "prettier --write '**/*.{js,ts,json,md}'",
    "lint-staged": "lint-staged",
    "build": "unbuild",
    "async": "npm run build && yalc push",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:cli:dev": "node ./lib/index.cjs dev",
    "test:cli:init": "node ./lib/index.cjs init"
  },
  "bin": {
    "sword": "./lib/index.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swordCodePractice/backend-cli.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/swordCodePractice/backend-cli/issues"
  },
  "homepage": "https://github.com/swordCodePractice/backend-cli#readme",
  "devDependencies": {
    "@types/download": "^8.0.1",
    "typescript": "^4.3.5",
    "unbuild": "^0.6.9"
  },
  "dependencies": {
    "@swc-node/register": "^1.4.2",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "consola": "^2.15.3",
    "download": "^8.0.0",
    "enquirer": "^2.3.6",
    "esbuild": "^0.14.27",
    "mri": "^1.2.0",
    "mv": "^2.1.1",
    "ohmyfetch": "^0.4.15",
    "tsbuffer-proto-generator": "^1.7.0",
    "unconfig": "^0.3.1"
  },
  "unbuild": {
    "entries": [
      "./src/index"
    ],
    "outDir": "lib",
    "declaration": true,
    "rollup": {
      "emitCJS": true,
      "cjsBridge": true
    }
  }
}
