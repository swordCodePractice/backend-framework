{
  "name": "@sword-code-practice/sword-framework",
  "version": "1.1.0",
  "description": "",
  "types": "./lib/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "exports": {
    "require": "./lib/index.cjs",
    "import": "./lib/index.mjs"
  },
  "scripts": {
    "dev": "unbuild --stub",
    "dev:nodemon": "nodemon --watch src/ -C -e ts --debug -x 'npm run async'",
    "prettier": "prettier --write '**/*.{js,ts,json,md}'",
    "lint-staged": "lint-staged",
    "build": "unbuild",
    "async": "npm run build && yalc push",
    "test": "vitest run",
    "publish:main": "pnpm publish --no-git-checks && open https://npmmirror.com/sync/@sword-code-practice/sword-framework",
    "publish:packages": "node scripts/publish-packages.mjs",
    "kill-port": "node scripts/kill-port.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swordCodePractice/sword-framework.git"
  },
  "keywords": [
    "serverless"
  ],
  "author": "Seho",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/swordCodePractice/sword-framework/issues"
  },
  "homepage": "https://github.com/swordCodePractice/sword-framework#readme",
  "devDependencies": {
    "@swc-node/register": "^1.4.2",
    "@types/node": "^14.18.12",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.2",
    "vitest": "latest"
  },
  "dependencies": {
    "@sword-code-practice/sword-plugin-log": "workspace:*",
    "@sword-code-practice/sword-plugin-server": "workspace:*",
    "h3": "0.7.3",
    "mri": "^1.2.0",
    "tsbuffer-validator": "^2.1.0"
  }
}
