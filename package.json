    "@sword-code-practice/types": "file:.yalc/@sword-code-practice/types",
    "@types/node": "^14.17.16",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
